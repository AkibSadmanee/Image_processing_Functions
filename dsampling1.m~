function[r]=dsampling1()
    a = imread('Board.bmp');
    [r, c] = size(a)
    
    a = a(1:2:end, 1:2:end);  
    
    imshow(a);
    
    %Upsample
    img=zeros(r,c);
    
    i=1; j=1; k=1; l=1;
    
    for x= 1:r/2
        for y= 1:c/2
            if j+1>=c/2
                break;
            end
            img(i,j) = a(k,l);
            img(i,j+1) = a(k,l);
            img(i+,j) = a(k,l);
            img(i+1,j+1) = a(k,l);
            
            l=l+1; j=j+2;
        end
        k=k+1; i=i+2;
        
        
end